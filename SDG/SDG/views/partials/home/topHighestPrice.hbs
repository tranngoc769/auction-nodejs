<div class="col-sm-4" >
    <div class="product-image-wrapper">
        <div class="single-products">
            <div class="productinfo text-center">
                <a href="/product/{{ID}}" >
                <img src="../../assets/images/9.png" alt="website template image">
                </a>
                <h4>{{ProName}}</h4><h5> {{curPrice}}</h5>
                <p>Sell Now</p>
                {{#if isLogin}}
                  <a href="#"  style="background: #fb8c00; color:#fff" class="btn btn-default add-to-cart"><i class="fa fa-shopping-cart"></i>Add to cart auction</a> 
                {{else}}
                  <a href="/login"  style="background: #fb8c00; color:#fff" class="btn btn-default add-to-cart"><i class="fa fa-shopping-cart"></i>Add to auction</a> 
                {{/if}}
            </div>
            <div class="product-overlay">
                <div  class="overlay-content">
                    <a href="/product/{{ID}}" >
                  <h4 style="color: #f9a825;">{{ProName}}</h4><h5 style="color: #f9a825;"> {{curPrice}}</h5>
                  <p style="color: #f9a825;">Sell Now</p>
                    </a>
                    {{#if isLogin}}
                  <a href="#" class="btn btn-default add-to-cart"><i class="fa fa-shopping-cart"></i>Add to auction</a> 
                    {{else}}
                  <a href="/login" class="btn btn-default add-to-cart"><i class="fa fa-shopping-cart"></i>Add to auction</a> 
                    {{/if}}
                   
                </div>
            </div>
        </div>
        <div class="choose">
            <ul class="nav nav-pills nav-justified">
                <li><a href="/login"><i class="fa fa-plus-square"></i>Bid Now</a></li>
            </ul>
        </div>
    </div>
</div>
